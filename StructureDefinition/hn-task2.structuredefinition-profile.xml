<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <url value="http://helsenorge.no/fhir/StructureDefinition/hn-task2" />
  <name value="HNTask2" />
  <status value="draft" />
  <fhirVersion value="4.0.0" />
  <mapping>
    <identity value="workflow" />
    <uri value="http://hl7.org/fhir/workflow" />
    <name value="Workflow Pattern" />
  </mapping>
  <mapping>
    <identity value="rim" />
    <uri value="http://hl7.org/v3" />
    <name value="RIM Mapping" />
  </mapping>
  <mapping>
    <identity value="w5" />
    <uri value="http://hl7.org/fhir/fivews" />
    <name value="FiveWs Pattern Mapping" />
  </mapping>
  <mapping>
    <identity value="v2" />
    <uri value="http://hl7.org/v2" />
    <name value="HL7 v2 Mapping" />
  </mapping>
  <kind value="resource" />
  <abstract value="false" />
  <type value="Task" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Task" />
  <derivation value="constraint" />
  <differential>
    <element id="Task.identifier">
      <path value="Task.identifier" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="system" />
        </discriminator>
        <rules value="open" />
      </slicing>
      <min value="1" />
    </element>
    <element id="Task.identifier:uuid">
      <path value="Task.identifier" />
      <sliceName value="uuid" />
      <short value="The unique identifier for this task instance" />
      <definition value="An unique identifier that identifies this task for the current citizen. Used as reference between an actor and helsenorge.no, as well as an internal reference on helsenorge.no." />
      <min value="1" />
      <max value="1" />
    </element>
    <element id="Task.identifier:uuid.system">
      <path value="Task.identifier.system" />
      <short value="The globally unique identifier for this task instance" />
      <min value="1" />
      <fixedUri value="urn:ietf:rfc:3986" />
    </element>
    <element id="Task.identifier:uuid.value">
      <path value="Task.identifier.value" />
      <min value="1" />
    </element>
    <element id="Task.instantiatesUri">
      <path value="Task.instantiatesUri" />
      <definition value="The URL pointing to an *externally* maintained  protocol, guideline, orderset or other definition that is adhered to in whole or in part by this Task.&#xD;&#xA;&#xD;&#xA;Helsenorge.no expects this, if used, to point to external applications like form fillers or other applications that can be used to seek fullfillment of the task." />
      <mustSupport value="true" />
    </element>
    <element id="Task.priority">
      <path value="Task.priority" />
      <definition value="Indicates how quickly the Task should be addressed with respect to other requests.&#xD;&#xA;&#xD;&#xA;Helsenorge.no uses this attribute internally to define rules for how to execute notifications and reminders for the citizen." />
      <mustSupport value="true" />
    </element>
    <element id="Task.description">
      <path value="Task.description" />
      <mustSupport value="true" />
    </element>
    <element id="Task.focus">
      <path value="Task.focus" />
      <mustSupport value="true" />
    </element>
    <element id="Task.focus.reference">
      <path value="Task.focus.reference" />
      <mustSupport value="true" />
    </element>
    <element id="Task.focus.display">
      <path value="Task.focus.display" />
      <definition value="Plain text narrative that identifies the resource in addition to the resource reference.&#xD;&#xA;&#xD;&#xA;Helsenorge.no uses the plain text narrative as a title for the task." />
      <mustSupport value="true" />
    </element>
    <element id="Task.authoredOn">
      <path value="Task.authoredOn" />
      <mustSupport value="true" />
    </element>
    <element id="Task.requester">
      <path value="Task.requester" />
      <min value="1" />
    </element>
    <element id="Task.requester.reference">
      <path value="Task.requester.reference" />
      <short value="Reference to a contained resource" />
      <definition value="A reference to a contained resource identifying the organization which requests the task to be performed." />
      <mustSupport value="true" />
    </element>
    <element id="Task.owner">
      <path value="Task.owner" />
      <definition value="Individual responsible for task execution." />
      <comment value="The requester must identify the owner of the, in helsenorge.no context this is a citizen." />
      <min value="1" />
    </element>
    <element id="Task.restriction">
      <path value="Task.restriction" />
      <mustSupport value="true" />
    </element>
    <element id="Task.restriction.extension">
      <path value="Task.restriction.extension" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="url" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="Task.restriction.extension:deadline">
      <path value="Task.restriction.extension" />
      <sliceName value="deadline" />
      <min value="0" />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://helsenorge.no/fhir/StructureDefinition/hn-task-deadline" />
      </type>
    </element>
    <element id="Task.restriction.period">
      <path value="Task.restriction.period" />
      <mustSupport value="true" />
    </element>
    <element id="Task.restriction.period.start">
      <path value="Task.restriction.period.start" />
      <mustSupport value="true" />
    </element>
    <element id="Task.restriction.period.end">
      <path value="Task.restriction.period.end" />
      <mustSupport value="true" />
    </element>
  </differential>
</StructureDefinition>